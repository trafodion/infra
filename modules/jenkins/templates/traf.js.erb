function makeDoubleDelegate(function1, function2) {
        return function() {
            if (function1)
                function1();
            if (function2)
                function2();
        }
}

function chgeLogo() {
        var traf=document.createElement("img");
        traf.setAttribute("id","trafodion-logo");
        traf.setAttribute("src","https://<%= vhost_name %>/userContent/title.png");
        traf.setAttribute("alt","trafodion");
        var jhl=document.getElementById("jenkins-home-link");
        jhl.insertBefore(traf,jhl.firstChild);
}

window.onload = makeDoubleDelegate(window.onload, chgeLogo);
